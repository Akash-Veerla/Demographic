# MongoDB & Server Config
MONGO_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/konnect_db?retryWrites=true&w=majority
JWT_SECRET=your_secure_random_jwt_secret_key_here_minimum_32_chars
PORT=5000

# URLs for CORS & API
# Start with http://localhost:5173 for local development
CLIENT_URL=http://localhost:5173

# Google OAuth (Passport Strategy)
GOOGLE_CLIENT_ID=your_google_client_id_from_cloud_console
GOOGLE_CLIENT_SECRET=your_google_client_secret_from_cloud_console
# Callback URL must match exactly what is in Google Cloud Console
# Default: http://localhost:5000/api/auth/google/callback
CALLBACK_URL=http://localhost:5000/api/auth/google/callback
